<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <defaults public="true" />

        <service id="referrer" alias="Base\Service\Referrer" />
        <service id="Base\Service\ReferrerInterface" alias="referrer"/>

        <service id="base.command" alias="Base\Console\Command" />

        <service id="base.service" alias="Base\Service\BaseService" />
        <service id="base.service.icon" alias="Base\Service\IconProvider" />
        <service id="base.service.sharer" alias="Base\Service\Sharer" />

        <service id="base.service.image" alias="Base\Service\ImageService" />
        <service id="Base\Service\ImageServiceInterface" alias="base.service.image"/>

        <service id="base.service.file" alias="Base\Service\FileService" />
        <service id="Base\Service\FileServiceInterface" alias="base.service.file"/>

        <service id="obfuscator" alias="Base\Service\Obfuscator" />
        <service id="Base\Service\ObfuscatorInterface" alias="obfuscator"/>

        <service id="base.service.maintenance" alias="Base\Service\MaintenanceProvider" />
        <service id="Base\Service\MaintenanceProviderInterface" alias="base.service.maintenance"/>

        <service id="base.service.maternity" alias="Base\Service\MaternityService" />
        <service id="Base\Service\MaternityServiceInterface" alias="base.service.maternity"/>

        <service id="base.filesystem" alias="Base\Service\Filesystem" />
        <service id="setting_bag" alias="Base\Service\Settings" />

        <service id="setting_bag" alias="Base\Service\SettingBag" />
        <service id="Base\Service\SettingBagInterface" alias="setting_bag"/>

        <service id="base.notifier" alias="Base\Notifier\Notifier" />
        <service id="Base\Notifier\NotifierInterface" alias="base.notifier"/>
        <service id="base.translator" alias="Base\Service\Translator" />
        <service id="Base\Service\TranslatorInterface" alias="base.translator"/>
        <service id="parameter_bag.hot" alias="Base\Service\ParameterBag" />
        <service id="Base\Service\HotParameterBagInterface" alias="parameter_bag.hot"/>
        <service id="base.widget_provider" alias="Base\Service\WidgetProvider" />
        <service id="Base\Service\WidgetProviderInterface" alias="base.widget_provider"/>
        <service id="base.paginator" alias="Base\Service\Paginator" />
        <service id="Base\Service\PaginatorInterface" alias="base.paginator"/>
        <service id="base.breadgrinder" alias="Base\Service\Breadgrinder" />
        <service id="Base\Service\BreadgrinderInterface" alias="base.breadgrinder"/>

        <service id="locale_provider" alias="Base\Service\LocaleProvider" />
        <service id="Base\Service\LocaleProviderInterface" alias="locale_provider"/>

        <service id="base.database.naming_strategy" alias="Base\Database\NamingStrategy" />
        <service id="base.database.metadata_factory" alias="Doctrine\Persistence\Mapping\ClassMetadataFactory" />
        <service id="base.database.entity_hydrator" alias="Base\Database\Factory\EntityHydrator" />
        <service id="base.database.metadata_manipulator" alias="Base\Database\Factory\ClassMetadataManipulator" />

        <service id="base.annotation_reader" alias="Base\Annotations\AnnotationReader" />

        <service id="base.entity_extension" alias="Base\Database\Factory\EntityExtension" />

        <service id="twig.extension.base"      alias="Base\Twig\Extension\BaseTwigExtension" />
        <service id="twig.loader.filesystem"   alias="Base\Twig\Loader\FilesystemLoader" />
        <service id="twig.random_variable"     alias="Base\Twig\Variable\RandomVariable" />
        <service id="twig.site_variable"       alias="Base\Twig\Variable\SiteVariable" />
        <service id="twig.backoffice_variable" alias="Base\Twig\Variable\BackofficeVariable" />
        <service id="twig.ea_variable"         alias="Base\Twig\Variable\EasyAdminVariable" />

        <service id="base.form.proxy"   alias="Base\Form\FormProxyInterface" />
        <service id="base.form.factory" alias="Base\Form\FormFactoryInterface" />

        <service id="ea_extension" alias="Base\Backend\Config\Extension" />

    </services>
</container>
