{
    "name": "xkzl/base-bundle",
    "type": "symfony-bundle",
    "description": "Base helper integration for Symfony",
    "keywords": ["symfony", "base", "helper"],
    "homepage": "https://gitlab.glitchr.io/repository-public/symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyerconde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Base\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Base\\": "./Tests"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/dependency-injection": "^4.0|^5.0",
        "symfony/http-kernel": "^4.0|^5.0",
        "symfony/config": "^4.0|^5.0",
        "scheb/2fa": "^5.0",
        "scheb/2fa-email": "^5.0",
        "scheb/2fa-qr-code": "^5.0",
        "scheb/2fa-trusted-device": "^5.0",
        "easycorp/easyadmin-bundle": "^3.5.0",
        "league/flysystem-bundle": "^2.0",
        "nelmio/security-bundle": "^2.10",
        "hashids/hashids": "^4.1",
        "hwi/oauth-bundle": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
