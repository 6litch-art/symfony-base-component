{
    "name": "xkzl/base-bundle",
    "type": "symfony-bundle",
    "description": "Base helper integration for Symfony",
    "keywords": ["symfony", "base", "helper"],
    "homepage": "https://gitlab.glitchr.io/repository-public/symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyerconde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Base\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Base\\": "./Tests"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "symfony/framework-bundle": "^4.0|^5.0|^6.0",
        "symfony/dotenv": "^4.0|^5.0|^6.0",
	"symfony/config": "^4.0|^5.0|^6.0",

        "symfony/http-client": "^4.0|^5.0|^6.0",
        "symfony/security-bundle": "^4.0|^5.0|^6.0",
        "symfony/form": "^4.0|^5.0|^6.0",
        "symfony/validator": "^4.0|^5.0|^6.0",
        "symfony/intl": "^4.0|^5.0|^6.0",
        "symfony/mailer": "^4.0|^5.0|^6.0",
        "symfony/messenger": "^4.0|^5.0|^6.0",
        "symfony/notifier": "^4.0|^5.0|^6.0",
        "symfony/uid": "^4.0|^5.0|^6.0",

        "symfony/dependency-injection": "^4.0|^5.0|^6.0",
        "symfony/discord-notifier": "^4.0|^5.0|^6.0",
        "symfony/monolog-bundle": "^3.1",

        "symfony/twig-bundle": "^4.0|^5.0|^6.0",
        "twig/twig": "^3.0",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/cssinliner-extra": "^3.0",
        "liip/imagine-bundle": "^2.7",

        "sensio/framework-extra-bundle": "^5.4",
        "scheb/2fa": "^5.0",
        "scheb/2fa-email": "^5.0",
        "scheb/2fa-qr-code": "^5.0",
        "scheb/2fa-trusted-device": "^5.0",

        "easycorp/easyadmin-bundle": "^4.0",
        "league/flysystem-bundle": "^2.0",
        "nelmio/security-bundle": "^2.10",
        "hashids/hashids": "^4.1",
        "hwi/oauth-bundle": "^1.4",

        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.0",
        "doctrine/dbal": "^3.2",

        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
