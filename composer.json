{
    "name": "glitchr/base-bundle",
    "type": "symfony-bundle",
    "description": "Base helper integration for Symfony",
    "keywords": ["symfony", "base", "helper"],
    "homepage": "https://gitlab.glitchr.dev/public-repository/symfony",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyer@glitchr.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Base\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Base\\": "./Tests"
        }
    },
    "require": {
        "php": "^8.0",

        "symfony/framework-bundle": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/config": "^6.0",

        "symfony/http-client": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/form": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/messenger": "^6.0",
        "symfony/notifier": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/discord-notifier": "^6.0",
        "symfony/monolog-bundle": "^3.1",

        "symfony/twig-bundle": "^6.0",
        "symfony/workflow": "^6.0",

        "scheb/2fa": "^6.0",
        "twig/twig": "^3.0",
        "twig/cache-extra": "^3.0",
        "twig/extra-bundle": "^3.0",
        "twig/intl-extra": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/cssinliner-extra": "^3.0",
        "imagine/imagine": "^1.2",
        "enshrined/svg-sanitize": "*",

        "endroid/qr-code": "^4.0",
        "picqer/php-barcode-generator": "^2.0",
        "florianv/swap": "^4.0",
        "scienta/doctrine-json-functions": "~4.3",
        "phpoffice/phpspreadsheet": "^1.23",
        "easycorp/easyadmin-bundle": "^4.4",
        "league/flysystem-bundle": "^2.4",
    	"league/flysystem-ftp": "^3.0",

        "nelmio/security-bundle": "^3.0",
        "nelmio/cors-bundle": "^2.2",
        "hashids/hashids": "^4.1",

        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/annotations": "^1.0",
        "glitchr/base-plugin": "1.0.*-dev",

    	"glitchr/backup-manager": "4.0.*-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/maker-bundle": "1.43",
        "wapmorgan/php-deprecation-detector": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "squizlabs/php_codesniffer": "^3.7",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "symfony/debug-bundle": "^6.2",
        "symfony/maker-bundle": "1.43",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "^6.2",
        "symfony/var-dumper": "^6.2",
        "symfony/web-profiler-bundle": "^6.2"
    },
    "config": {
        "preferred-install": {"*": "dist"},
        "sort-packages": true
    }
}
