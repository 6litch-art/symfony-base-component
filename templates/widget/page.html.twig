{% extends 'layout.html.twig' %}

{% block title %}{{ page.title }}{% endblock %}
{% block content %}
<article>
    <h1>{{ page.title }}</h1>
    <h3>{{ page.excerpt }}</h3>

    <p>{{page.content|raw}}</p>

    <div class="page-referer">{{ '@messages.base.return'|trans([app.referrer.getUrl()])|raw }}<p>
</article>
{% endblock %}
