{% extends 'layout.html.twig' %}

{% block title %}{{ page.title }}{% endblock %}
{% block content %}
<article>
    <h1>{{ page.title }}</h1>
    <h3>{{ page.excerpt }}</h3>

    <p>{{page.content|raw}}</p>

    <a href="{{ app.referrer.getUrl() }}">Retour à la page précédente</a>
</article>
{% endblock %}
