{% extends 'email.html.twig' %}

{% block subject %}
    {{ "emails.adminApproval.subject"|trans2|raw }}
{% endblock %}

{% block content %}
    {{ "emails.adminApproval.content"|trans2([
        user.getUsername(), 
        user.getId()
    ])|raw }}
{% endblock %}

{% block action_text %}
    {{ "emails.adminApproval.action_text" | raw }}
{% endblock %}

{% block action_url %}
    {{ path("base_dashboard") }}
{% endblock %}
