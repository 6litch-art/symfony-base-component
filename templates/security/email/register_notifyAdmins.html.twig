{% extends 'email.html.twig' %}

{% block subject %}
    {{ '@emails.security.register_notifyAdmins.subject'|trans|raw }}
{% endblock %}

{% block content %}
    {{ '@emails.security.register_notifyAdmins.content'|trans([newUser, newUser.email])|raw }}
{% endblock %}

{% block action_text %}
    {{ '@emails.security.register_notifyAdmins.action_text'|trans([newUser|trans_entity]) | raw }}
{% endblock %}

{% block action_url %}
    {{ newUser|linkify }}
{% endblock %}