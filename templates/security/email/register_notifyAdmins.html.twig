{% extends 'email.html.twig' %}

{% block subject %}
    {{ '@emails.register_notifyAdmins.subject'|trans|raw }}
{% endblock %}

{% block content %}
    {{ '@emails.register_notifyAdmins.content'|trans([new_user, new_user.email])|raw }}
{% endblock %}

{% block action_text %}
    {{ '@emails.register_notifyAdmins.action_text'|trans([new_user|trans_entity]) | raw }}
{% endblock %}

{% block action_url %}
    {{ new_user|linkify }}
{% endblock %}